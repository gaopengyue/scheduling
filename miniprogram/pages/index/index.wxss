page {
  background: #fff;
  min-height: 100vh;
}
.logo {
  display: flex;
  justify-content: center;
}
.logo image {
  display: block;
  width: 100%;
  height: 82rpx;
  margin-top: -2rpx;
}

.list {
  padding-top: 50rpx;
}
.list navigator {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  border-bottom: 1rpx solid #efefef;
  padding: 28rpx;
  background: url(data:img/jpg;base64,) no-repeat center right 24rpx;
  background-size: 54rpx;
}
.list image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 24rpx;
  border: 2rpx solid #efefef;
}
.btm {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 60rpx;
}