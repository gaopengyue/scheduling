page {
  background: #fff;
  min-height: 100vh;
}
.logo {
  display: flex;
  align-items: center;
  font-size: 36rpx;
}
.logo image {
  display: block;
  width: 130rpx;
  height: 130rpx;
  padding-right: 10rpx;
}

.list {
  padding-top: 50rpx;
}
.list navigator {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  border-bottom: 1rpx solid #efefef;
  padding: 28rpx;
  background: url(data:img/jpg;base64,) no-repeat center right 24rpx;
  background-size: 54rpx;
}
.list image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 24rpx;
  border: 2rpx solid #efefef;
}
.empty {
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  color: #666;
  font-size: 30rpx;
  height: 700rpx;
}
.empty image {
  width: 100rpx;
  height: 100rpx;
  opacity: .5;
  padding-bottom: 51rpx;
}
.btm {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 60rpx;
}